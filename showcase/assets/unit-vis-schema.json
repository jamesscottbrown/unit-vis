                                },
                                "type": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "shape": {
                            "enum": [
                                "circle",
                                "rect"
                            ],
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "padding": {
                    "$ref": "#/definitions/Padding",
                    "description": "Size of padding internal padding for layout, defaults to zeroes"
                },
                "title": {
                    "description": "The title of the rendered chart, currently unused",
                    "type": "string"
                },
                "width": {
                    "description": "The width of the output",
                    "type": "number"
                }
            },
            "type": "object"
        }
